pragma solidity ^0.4.24;
 
contract BusinessCard {
    
    string name;
    uint age;
    
    function getName() public view returns (string) {
        return name;
    }
    
    function setName(string newName) public {
        name = newName;
    }
    
    function getAge() public view returns (uint) {
        return age;
    }
    
    function setAge(uint newAge) public {
        age = newAge;
    }
 
}
